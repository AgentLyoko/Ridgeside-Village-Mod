			"Name": "IreneTraveling",
			"Value": true,
			"When": { "Query: ({{Season|contains=spring,summer}} AND {{Day|contains=15, 16, 17, 18, 19, 20, 21}}) OR ({{Season|contains=fall,winter}} AND {{Day|contains=2, 3, 4, 5, 6, 7}})": true }
